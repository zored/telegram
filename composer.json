{
    "name": "zored/telegram",
    "description": "PHP Telegram API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob",
            "email": "i@ro.gl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zored\\Telegram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zored\\Telegram\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "danog/madelineproto": "dev-master#a18cfa28efa4",
        "jms/serializer": "^1.0",
        "league/flysystem": "^1.0",
        "paillechat/php-enum": "^1.1"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/danog/phpseclib"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "nette/php-generator": "^3.1",
        "php-coveralls/php-coveralls": "^2.0",
        "phpunit/phpunit": "^7.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "config": {
        "sort-packages": true
    }
}
